{
    "name": "causewecan/website",
    "description": "Guild website with Discord SSO, blog, calendar and Stormforge roster integration.",
    "type": "project",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/socialite": "^5.15",
        "socialiteproviders/discord": "^4.5",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "dev": [
            "php artisan serve"
        ]
    }
}
